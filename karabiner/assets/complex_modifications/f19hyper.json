{
    "title": "Change f19",
    "rules": [

      {
        "description": "Change f19 key to command+control+option+shift.",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "f19",
              "modifiers": {
                "optional": [
                  "any"
                ]
              }
            },
            "to": [
              {
                "key_code": "left_shift",
                "modifiers": [
                  "left_command",
                  "left_control",
                  "left_option"
                ]
              }
            ]
           
          }
        ]
      }
  
    ]
  }
  